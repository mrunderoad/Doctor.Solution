@{
  Layout = "_Layout";
}

@model DoctorsOffice.Models.Specialty

<div class="jumbotron">
  <h1>Edit @Html.DisplayFor(model => model.SpecialtyName)</h1>
</div>

@using (Html.BeginForm())
{
  @Html.HiddenFor(model => model.SpecialtyId)
  @Html.Label("Specialty Name:")
  @Html.EditorFor(model => model.SpecialtyName)
  <br>
  <input type="submit" value="Update" />
}